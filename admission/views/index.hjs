<!DOCTYPE html>
<html><!-- <html ng-app="ng.admission"> -->
<head>
	<title>AdMission - Advertising Matters</title>
	<link rel="stylesheet" type="text/css" href="/stylesheets/reset.css">
	<link rel="stylesheet" type="text/css" href="/stylesheets/sample.css">
	<link rel="stylesheet" type="text/css" href="/stylesheets/style.css">
	<link rel="stylesheet" type="text/css" href="/stylesheets/bootstrap.css">
	<link rel="stylesheet" type="text/css" href="/stylesheets/hover.css">
	<link rel="stylesheet" href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">
</head>
<body>
	<div id="mainWrapper">
		<div id="headerWrapper">
			<div id="signupWrapper">
				<div id="signupBox">
					<img class="hvr-float-shadow" src="/images/Anotherlogo.png" width='250'>
					<div class="arrow-left"></div> 
				</div>
			</div>
			<div id="mainHeaderWrapper">
				<div id="menuHeader">
					<!-- Sign Up Link-->
					<div class="menuOption menu_first hvr-grow"> <span class="menu_text"><a href ="/signup"> <img class="menuIcon" src="/images/signup.png" width='30' height='30'/ >Sign Up </span></div></a>
					<!-- Log In Link-->
					<div class="menuOption hvr-grow"> <span class="menu_text"> <a href ="/login"><img class="menuIcon" ng-click="open()" src="/images/signin.png" width='30' height='30'/>Log In </span></div></a>
					<!-- Trending Link-->
					<div class="menuOption hvr-grow"> <span class="menu_text"> <a href ="/trending"><i class="fa fa-star"></i>Trending </span></div></a>
					<!-- About Link--> 
					<div class="menuOption hvr-grow"> <span class="menu_text"> <a href ="/about"><img class="menuIcon" src="/images/about.png" width='30' height='30'/>About </span></div></a>
				</div>
			</div>
		</div>

		
		<!-- <div ui-view></div> -->

		<div class"injection">{{> part}}</div>

		
	</div>
	<footer class="footer"></footer>
	</body>

	<script src="http://code.jquery.com/jquery-1.11.2.min.js"></script>
	<script type="text/javascript" src="/javascripts/sample.js"></script>
	<script type="text/javascript" src="/javascripts/angular.js"></script>
	<script type="text/javascript" src="/javascripts/lodash.js"></script>
	<script type="text/javascript" src="/javascripts/underscore.js"></script>
	<script type="text/javascript" src="/angular/modules.js"></script>
	<script type="text/javascript" src="/javascripts/ui-bootstrap.js"></script>


	

</html>